	<animation>
		<type>translate</type>
		<object-name>TrennDoor</object-name>
		<object-name>DoorKnob</object-name>
		<property>sim/model/door-positions/TDoor/position-norm</property>
		<interpolation>
			<entry>
				<ind>0.0</ind>
				<dep>0.0</dep>
			</entry>
			<entry>
				<ind>1</ind>
				<dep>-0.5</dep>
			</entry>
		</interpolation>
		<axis>
			<x>0</x>
			<y>1</y>
			<z>0</z>
		</axis>
	</animation>

	<animation>
		<type>pick</type>
		<object-name>DoorKnob</object-name>
		<action>
			<button>0</button>
			<repeatable>false</repeatable>
			<binding>
				<command>nasal</command>
				<script>L30.openCabinDoors();</script>
			</binding>
		</action>
	</animation>

