<?xml version="1.0"?>

<PropertyList> 
	
	<path>switch.ac</path>
	<parameters>
		<state></state>
	</parameters>
	
	<offsets>
		<!-- x/y/z == back/right/up -->
		<x-m> 0.000 </x-m>
		<y-m> 0.000 </y-m>
		<z-m> 0.000 </z-m>
	</offsets>
	
	<animation>
		<type>pick</type>
		<object-name>Switch</object-name>
		<action>
			<button>0</button>
			<repeatable>false</repeatable>
			<binding>
				<command>property-toggle</command>
				<property alias="../../../../parameters/state" />
			</binding>
		</action>
	</animation>
	<animation>
		<type>rotate</type>
		<object-name>Switch</object-name>
		<property alias="../../parameters/state" />
		<factor>90</factor>
		<center>
			<x-m>  0.00 </x-m>
			<y-m>  0.00 </y-m>
			<z-m>  0.00 </z-m>
		</center>
		<axis>
			<x>0.0</x>
			<y>0.0</y>
			<z>1.0</z>
		</axis>
	</animation>
	
</PropertyList>
